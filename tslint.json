{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "double"],
    "import-name": false,
    "no-namespace": false,
    "interface-name": false,
    "no-empty-interface": false,
    "no-boolean-literal-compare": true
  },
  "rulesDirectory": [
    "src"
  ]
}